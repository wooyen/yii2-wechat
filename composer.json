{
	"name": "wooyen/yii2-wechat",
	"description": "Wechat SDK for Yii2",
	"type": "yii2-extension",
	"require": {
		"php": ">=8.2",
		"ext-curl": "*",
		"yiisoft/yii2": "~2.0.36"
	},
	"require-dev": {
		"yiisoft/yii2-redis": "~2.0.0"
	},
	"suggest": {},
	"license": "GPL-3.0-or-later",
	"autoload": {
		"psr-4": {
			"yii\\wechat\\": "src/"
		}
	},
	"extra": {
		"bootstrap": "yii\\wechat\\Bootstrap"
	},
	"authors": [
		{
			"name": "WU Yan",
			"email": "wooyen@163.com"
		}
	],
	"repositories": {
		"asset-packagist": {
			"type": "composer",
			"url": "https://asset-packagist.org"
		}
	},
	"config": {
		"allow-plugins": {
			"yiisoft/yii2-composer": true
		}
	}
}